  },
  "dependencies": {
    "fastify": "^2.15.1",
    "@mia-platform/custom-plugin-lib": "^2.1.1"
  },
  "devDependencies": {
    "@mia-platform/eslint-config-mia": "^3.0.0",
    "eslint": "^7.31.0",
    "nock": "^13.0.4",
    "tap": "^14.10.8"
  },
  "private": true,
  "eslintConfig": {
    "extends": "@mia-platform/eslint-config-mia"
  }
}
