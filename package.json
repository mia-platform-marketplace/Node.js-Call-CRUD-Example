  "main": "index.js",
  "scripts": {
    "checkonly": "! grep -R '\\.only' tests/",
    "coverage": "npm run unit -- --coverage-report=text-summary",
    "postcoverage": "tap --coverage-report=lcov --no-browser",
    "lint": "eslint . --ignore-path .gitignore",
